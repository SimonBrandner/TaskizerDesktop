{
	"name": "taskizer-desktop",
	"version": "1.0.0-alpha",
	"main": "electron/main.js",
	"author": "Å imon Brandner",
	"build": {
		"appId": "taskizer-desktop"
	},
	"scripts": {
		"ng": "ng",
		"test": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"run-angular": "ng serve",
		"build-angular": "ng build",
		"run-electron": "electron . --no-sandbox",
		"build-angular-and-run-electron": "ng build && electron . --no-sandbox",
		"prod-build-linux": "ng build && electron-packager . --platform=linux --out=build/ --overwrite --production --ignore=/e2e",
		"prod-build-win32": "ng build && electron-packager . --platform=win32 --out=build/ --overwrite --production --ignore=/e2e"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^10.0.1",
		"@angular/cdk": "^10.0.0",
		"@angular/common": "^10.0.1",
		"@angular/compiler": "^10.0.1",
		"@angular/core": "^10.0.1",
		"@angular/forms": "^10.0.1",
		"@angular/localize": "^10.0.1",
		"@angular/material": "^10.0.0",
		"@angular/platform-browser": "^10.0.1",
		"@angular/platform-browser-dynamic": "^10.0.1",
		"@angular/router": "^10.0.1",
		"jsonpath": "^1.0.2",
		"rxjs": "~6.5.5",
		"tslib": "^2.0.0",
		"zone.js": "~0.10.3"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~0.1000.0",
		"@angular/cli": "^10.0.0",
		"@angular/compiler-cli": "^10.0.1",
		"@angular/language-service": "^10.0.1",
		"@types/jasmine": "^3.5.11",
		"@types/jasminewd2": "~2.0.3",
		"@types/node": "^12.12.47",
		"codelyzer": "^5.2.2",
		"electron": "^9.0.5",
		"electron-packager": "^15.0.0",
		"jasmine-core": "~3.5.0",
		"jasmine-spec-reporter": "~5.0.0",
		"karma": "~5.0.0",
		"karma-chrome-launcher": "~3.1.0",
		"karma-coverage-istanbul-reporter": "~3.0.2",
		"karma-jasmine": "~3.3.0",
		"karma-jasmine-html-reporter": "^1.5.4",
		"protractor": "~7.0.0",
		"ts-node": "~8.3.0",
		"tslint": "~6.1.0",
		"typescript": "~3.9.5"
	}
}
